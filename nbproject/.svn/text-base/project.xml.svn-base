<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>GEVA-piGE</name>
            <properties/>
            <folders>
                <source-folder>
                    <label>GEVA/src</label>
                    <type>java</type>
                    <location>GEVA/src</location>
                </source-folder>
                <source-folder>
                    <label>GUI/src</label>
                    <type>java</type>
                    <location>GUI/src</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>build</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="test">
                    <target>test</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>build</target>
                </action>
                <action name="run">
                    <target>all</target>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>GEVA/src</label>
                        <location>GEVA/src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>GUI/src</label>
                        <location>GUI/src</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="run"/>
                    <ide-action name="test"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>GEVA/src</package-root>
                <classpath mode="compile">bin/lib/antlr/antlr-2.7.5.jar:bin/lib/asm/asm-2.2.jar:bin/lib/bsf/bsf.jar:bin/lib/commons-logging/commons-logging-1.1.jar:bin/lib/groovy/groovy-1.0.jar:bin/lib/jscheme/jscheme-7.2.jar:bin/lib/jsci/jsci-core.jar:bin/lib/jsci/jsci-xtra.jar</classpath>
                <source-level>1.5</source-level>
            </compilation-unit>
            <compilation-unit>
                <package-root>GUI/src</package-root>
                <source-level>1.5</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
